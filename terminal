# 创建项目目录
mkdir devtoolbox
cd devtoolbox

# 初始化 package.json
npm init -y

# 安装核心依赖
npm install vue@next typescript @vitejs/plugin-vue vite --save-dev
npm install element-plus @element-plus/icons-vue pinia vue-router --save
npm install @types/node @typescript-eslint/parser @typescript-eslint/eslint-plugin --save-dev
npm install eslint eslint-plugin-vue prettier --save-dev 